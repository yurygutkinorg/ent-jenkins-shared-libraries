<settings>
    <servers>
        <server>
            <id>ghi-artifactory</id>
            <username><![CDATA[${ARTIFACTORY_USERNAME}]]></username>
            <password><![CDATA[${ARTIFACTORY_PASSWORD}]]></password>
        </server>
        <server>
            <id>ghi-exchange</id>
            <username><![CDATA[${ANYPOINT_USERNAME}]]></username>
            <password><![CDATA[${ANYPOINT_PASSWORD}]]></password>
        </server>
        <server>
            <id>MuleRepository</id>
            <username><![CDATA[${MULE_REPOSITORY_USERNAME}]]></username>
            <password><![CDATA[${MULE_REPOSITORY_PASSWORD}]]></password>
        </server>
    </servers>
    <profiles>
        <profile>
            <id>Mule</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <repositories>
                <repository>
                    <id>MuleRepository</id>
                    <name>MuleRepository</name>
                    <url>https://repository.mulesoft.org/nexus-ee/content/repositories/releases-ee/</url>
                    <layout>default</layout>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </repository>
            </repositories>
        </profile>
    </profiles>
</settings>